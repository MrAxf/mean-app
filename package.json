    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.2"
  }
}
